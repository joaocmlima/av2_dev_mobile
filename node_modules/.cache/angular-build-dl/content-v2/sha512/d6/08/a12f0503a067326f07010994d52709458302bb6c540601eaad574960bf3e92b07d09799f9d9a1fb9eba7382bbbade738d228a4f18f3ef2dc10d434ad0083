let NewsApiService = class NewsApiService {
    constructor(http) {
        this.http = http;
        this.chave = "a192006ac51443b085001c4a7485c2e0";
        this.caminhoPadrao = "https://newsapi.org/v2";
    }
    getEverythingAbout(assunto, page = 1, resultsPerPage = 20) {
        let result = `${this.caminhoPadrao}/everything?q=${assunto}&pageSize=${resultsPerPage}&language=pt&page=${page}&apiKey=${this.chave}`;
        console.log(result);
        return this.http.get(result);
    }
};
